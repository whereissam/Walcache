{
  "name": "@wcdn/cli",
  "version": "1.0.0",
  "description": "Command-line interface for WCDN (Walrus Content Delivery Network)",
  "main": "dist/index.js",
  "bin": {
    "wcdn": "dist/cli.js",
    "wcdn-cli": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "walrus",
    "cdn",
    "cli",
    "blockchain",
    "decentralized-storage",
    "wcdn"
  ],
  "author": "WCDN Team",
  "license": "MIT",
  "dependencies": {
    "@wcdn/sdk": "^1.0.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "table": "^6.8.1",
    "fs-extra": "^11.1.1",
    "mime-types": "^2.1.35",
    "progress": "^2.0.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.0",
    "@types/progress": "^2.0.7",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}