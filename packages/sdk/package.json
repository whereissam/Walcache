{
  "name": "@wcdn/sdk",
  "version": "1.0.0",
  "description": "WCDN Developer SDK - Easy integration with Walrus CDN",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "walrus",
    "cdn",
    "web3",
    "storage",
    "decentralized",
    "cache"
  ],
  "author": "WCDN Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "vitest": "^1.1.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/wcdn.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/wcdn/issues"
  },
  "homepage": "https://github.com/your-org/wcdn#readme"
}