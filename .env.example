# Server Configuration
PORT=3000
NODE_ENV=development

# Walrus Configuration
WALRUS_ENDPOINT=https://publisher.walrus.wal.app
WALRUS_AGGREGATOR=https://aggregator.walrus.wal.app

# Caching Configuration
REDIS_URL=redis://localhost:6379
CACHE_TTL=3600
MAX_CACHE_SIZE=1000

# Security
API_KEY_SECRET=your-secret-key-here
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Analytics
ENABLE_ANALYTICS=true
WEBHOOK_URL=

# Tusky.io Configuration
TUSKY_API_URL=https://api.tusky.io
TUSKY_API_KEY=your-tusky-api-key-here
TUSKY_DEFAULT_VAULT_ID=